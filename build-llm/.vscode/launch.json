    "version": "0.2.0",
    "configurations": [
    
        {
            "name": "Python Debugger: Current File (.uv-env)",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "python": "${workspaceFolder}/.uv-env/bin/python"
        }
    ]
}